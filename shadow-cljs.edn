{:nrepl {:port 8777}

 :source-paths ["src" "test"]

 :dependencies
 [[reagent "1.1.0"]
  [re-frame "1.2.0"]
  [day8.re-frame/tracing "0.6.2"]
  [day8.re-frame/re-frame-10x "1.2.0"]
  [cljsjs/moment "2.24.0-0"]
  [re-com "2.13.2"]
  [re-graph "0.1.16"]
  [herb "0.10.0"]
  [rgm/tailwind-hiccup "0.2.0"]
  [bidi "2.1.6"]
  [kibu/pushy "0.3.8"]
  [binaryage/devtools "1.0.4"]]

 :dev-http
 {8280 "resources/public"
  8290 "target/browser-test"}

 :builds
 {:app
  {:target :browser
   :output-dir "resources/public/js/compiled"
   :asset-path "/js/compiled"
   :modules
   {:app {:init-fn app.core/init}}
   :devtools
   {:preloads [day8.re-frame-10x.preload]}
   :dev {:compiler-options
         {:closure-defines
          {re-frame.trace.trace-enabled? true
           day8.re-frame.tracing.trace-enabled? true
           re-com.config/root-url-for-compiler-output "http://localhost:8290/js/compiled/app/cljs-runtime/"}}}
   :prod {:optimizations :advanced
          :pretty-print false}}
  :browser-test
  {:target    :browser-test
   :ns-regexp "-test$"
   :runner-ns shadow.test.browser
   :test-dir  "target/browser-test"}
  :karma-test
  {:target    :karma
   :ns-regexp "-test$"
   :output-to "target/karma-test.js"}}}
